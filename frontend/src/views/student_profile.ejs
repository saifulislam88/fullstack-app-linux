<% function layout(name){ include(name) } %>
<section class="card"><h2>My Profile</h2><div class="profile"><img class="avatar" src="<%= me.photo_url %>" alt="Profile picture"><div><p><strong>Name:</strong> <%= me.name %></p><p><strong>Email:</strong> <%= me.email %></p><p><strong>Institute:</strong> <%= me.institute %></p><p><strong>Batch:</strong> <%= me.batch %></p><p><strong>Course:</strong> <%= me.course_name %></p><p><strong>Module:</strong> <%= me.module %></p></div></div></section>